{
  "name": "ao-fetch",
  "type": "module",
  "version": "1.0.0",
  "repository": {
    "url": "https://github.com/ankushKun/aoxpress",
    "type": "git"
  },
  "description": "tool to communicate with ao processes in a familiar http like way",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
    "build": "tsc && npm run build:browser",
    "build:browser": "esbuild src/index.ts --bundle --format=iife --global-name=aofetch --outfile=dist/browser.js",
    "publish": "pnpm run build && npm publish --provenance --access public"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@permaweb/aoconnect": "^0.0.82",
    "arweave": "^1.15.7",
    "typescript": "^5.8.3",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "esbuild": "^0.20.2"
  }
}